<!--Main-Body-Start-->
<div class="main-body-container">
  <div class="main-body">

    <div class="main-body-content">

      <div id="system-message-container">
      </div>

      <table>
        <% @video.each do |video| %>
          <tr>
            <td>
              <%= link_to image_tag("thumb/"+video.thumb_name, :size => "200x150"), video,'data-no-turbolink' => true    %>
            </td>
            <td class="videotitle">
              <h3><%= link_to video.title, video,'data-no-turbolink' => true %></h3><br><br>
              <h6>Created On : <%= video.created_at.strftime("%m/%d/%Y") %><h6>
                  </td></tr>
                <% end %>
                </table>
                </div>
                </div>
                <%= will_paginate @video %>
                <!--Footer-Start-->
                <div class="footer-container">
                  <div class="footer">
                    <div class="footer-content">
                      <div class="footer-content-complete">
                        <h4>Footer info?</h4>	
                      </div>
                    </div>
                  </div>
                </div>
                <!--Footer-End-->
                </body>
                </html>

